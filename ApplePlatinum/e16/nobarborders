#include <definitions>

__E_CFG_VERSION 0

/* ********* HOW TO USE THIS FILE ******************** *
***
*** Use this replacement for borders.cfg to let windows 
*** maximize past the macbar at the top. Until Enlightenment
*** is fixed, there is no way to let windows cover buttons,
*** like the macbar, so you should turn buttons off with
*** Ctrl-Alt-B when using this file so you can see the 
*** titlebars of maxed windows.
***
*** rename the default borders.cfg to barborders and renames 
*** this file, nobarborders, to borders.cfg. DO NOT GIVE 
*** THE UNUSED FILE THE *.CFG EXTENSION! This will stop eMac
*** from working, thanks to a recent feature added to e.
***
****************************************************** */

__BORDER __BGN
  __NAME BORDERLESS
  __BORDER_SIZE_LEFT 0
  __BORDER_SIZE_RIGHT 0
  __BORDER_SIZE_TOP 0
  __BORDER_SIZE_BOTTOM 0
__END

/* *********************************** DEFAULT WINDOW ******************* */

__BORDER __BGN
  __NAME DEFAULT
	__BORDER_SIZE_LEFT 6
	__BORDER_SIZE_RIGHT 7
	__BORDER_SIZE_TOP 22
	__BORDER_SIZE_BOTTOM 7
	__SHADE_DIRECTION __UP
	__CHANGES_SHAPE __OFF

	__BORDER_PART __BGN
	  __NAME TITLE_SPACE
	  __ICLASS TITLE_SPACING
	  __ACLASS MOVE_WINDOW
	  __MIN_WIDTH 0
	  __MAX_WIDTH 99999
	  __MIN_HEIGHT 22
	  __MAX_HEIGHT 22
